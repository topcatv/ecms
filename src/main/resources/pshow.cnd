[ps:file] > nt:hierarchyNode, mix:lastModified, mix:versionable
  + ps:content (nt:base) primary mandatory
  + ps:copy (nt:base)
  - ps:size (long)
  - ps:name (string) mandatory
  - ps:mimeType (string)
  - ps:encoding (string)
[ps:folder] > nt:hierarchyNode, mix:lastModified
  + * (nt:hierarchyNode) VERSION
  - ps:name (string) mandatory